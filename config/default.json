{
  "domain": "wp-framework.dev",
  "theme": "wp-framework",
  "path": {
    "base": {
      "src":"assets/",
      "dest": "./html/"
    },
    "images": {
      "src": "assets/img/",
      "srcimg": "assets/img/**/*.{png,jpg,jpeg,gif,svg}",
      "dest": "./html/img/"
    },
    "fonts": {
      "src": "assets/fonts/**",
      "dest": "./html/fonts/"
    }
  }
}
