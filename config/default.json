{
  "domain": "wp-framework.dev",
  "theme": "wp-framework",
  "path": {
    "base": {
      "src":"assets/",
      "dest": "./html/"
    },
    "images": {
      "src": "assets/img/",
      "srcimg": "assets/img/**/*.{png,jpg,jpeg,gif,svg}",
      "dest": "./html/img/",
      "sprite": "assets/sprite/*"
    },
    "fonts": {
      "src": "assets/fonts/**",
      "dest": "./html/fonts/"
    },
    "styles": {
      "src": "assets/scss/",
      "srcfiles": "assets/scss/**/*.scss",
      "css": "assets/css/",
      "cssfiles": "assets/css/**/*.css",
      "dest": "./html/css/"
    }
  }
}
