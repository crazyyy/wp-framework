{
  "domain": "wpeb.ddev.site",
  "theme": "wp-wpeb",
  "ssl": {
    "key": "c:\\apps\\persist\\laragon\\etc\\ssl\\laragon.key",
    "cert": "c:\\apps\\persist\\laragon\\etc\\ssl\\laragon.crt"
  },
  "path": {
    "base": {
      "src":"assets/",
      "dest": "./html/",
      "destHtml": "./html/**/*.{html,htm,php}"
    },
    "images": {
      "src": "assets/img/",
      "srcImg": "assets/img/**/*.{png,jpg,jpeg,gif,svg,webp}",
      "srcImgContent": "wp-content/uploads/",
      "dest": "./html/img/"
    },
    "fonts": {
      "src": "assets/fonts/**",
      "dest": "./html/fonts/"
    },
    "styles": {
      "src": "assets/scss/",
      "srcFiles": "assets/scss/**/*.scss",
      "css": "assets/css/",
      "cssFiles": "assets/css/**/*.css",
      "dest": "./html/css/"
    },
    "scripts": {
      "src": "assets/js/**",
      "dest": "./html/js/"
    }
  }
}
