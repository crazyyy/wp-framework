{
  "wp_theme_name": "wp-framework",
  "wp_site_name": "wp-framework.dev",
  "name": "wp-framework",
  "version": "2016.07.24",
  "description": "simple html and css framework. before run you need to install 'npm install -g gulp browserify babel jade stylus http-server'",
  "homepage": "https://github.com/crazyyy/wp-framework#readme",
  "author": {
    "name": "Vitali Antoniuk",
    "email": "crazyyy@gmail.com",
    "url": "http://saitobaza.ru"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazyyy/wp-framework.git"
  },
  "bugs": {
    "url": "https://github.com/crazyyy/wp-framework/issues"
  },
  "main": "html/index.html",
  "keywords": [
    "html",
    "css",
    "wordpress",
    "ES5",
    "ES6",
    "JS",
    "Gulp"
  ],
  "browser": {},
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "browserify-shim": {},
  "scripts": {
    "start": "SET NODE_ENV=development & gulp",
    "build": "NODE_ENV=production gulp build",
    "start-wp": "NODE_ENV=development_wp gulp serve",
    "build-wp": "NODE_ENV=production_wp gulp build"
  },
  "lintOptions": {
    "globals": {
      "React": false
    },
    "esnext": true,
    "node": true,
    "bitwise": true,
    "browser": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "strict": false,
    "globalstrict": true,
    "sub": true,
    "supernew": true,
    "undef": true,
    "unused": true
  },
  "dependencies": {
    "jquery": "^2.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.1",
    "browserify-shim": "^3.8.12",
    "css-mqpacker": "^4.0.1",
    "cssnext": "^1.8.4",
    "csswring": "^4.2.2",
    "del": "^2.2.1",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-base64": "^0.1.3",
    "gulp-cache": "^0.4.3",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-csso": "^1.1.0",
    "gulp-derequire": "^2.1.0",
    "gulp-filter": "^4.0.0",
    "gulp-header": "^1.8.7",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.2",
    "gulp-jade": "^1.1.0",
    "gulp-jade-inheritance": "^0.5.5",
    "gulp-jshint": "^2.0.1",
    "gulp-load-plugins": "^1.2.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-newer": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "gulp-wait": "0.0.2",
    "gulp-webpack": "^1.5.0",
    "gulp.spritesmith": "^6.2.0",
    "imagemin-pngquant": "^4.2.0",
    "jshint": "^2.9.2",
    "map-stream": "0.0.6",
    "node-notifier": "^4.6.0",
    "reactify": "^1.1.1",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-named": "^1.1.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack": "^1.12.14",
    "webpack-stream": "^3.1.0"
  }
}
