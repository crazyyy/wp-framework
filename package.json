    "modernizr": "^3.13.1",
    "rfs": "^10.0.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.28.3",
