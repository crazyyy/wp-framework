{
  "wp_theme_name": "wp-framework",
  "wp_site_name": "wp-framework.dev",
  "name": "wp-framework",
  "version": "2016.07.24",
  "description": "simple html and css framework. before run you need to install 'npm install -g gulp browserify babel jade stylus http-server'",
  "homepage": "https://github.com/crazyyy/wp-framework#readme",
  "author": {
    "name": "Vitali Antoniuk",
    "email": "crazyyy@gmail.com",
    "url": "http://saitobaza.ru"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazyyy/wp-framework.git"
  },
  "bugs": {
    "url": "https://github.com/crazyyy/wp-framework/issues"
  },
  "main": "html/index.html",
  "keywords": [
    "html","css","wordpress","ES5","ES6","JS","Gulp"
  ],
  "browser": {},
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "browserify-shim": {},
  "scripts": {
    "start": "SET NODE_ENV=development & gulp serve",
    "build": "SET NODE_ENV=production & gulp serve"
  },
  "lintOptions": {
    "globals": {
      "React": false
    },
    "esnext": true,
    "node": true,
    "bitwise": true,
    "browser": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "strict": false,
    "globalstrict": true,
    "sub": true,
    "supernew": true,
    "undef": true,
    "unused": true
  },
  "dependencies": {
    "jquery": "^2.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "autoprefixer-core": "^6.0.1",
    "babel-preset-es2015": "^6.9.0",
    "browser-sync": "^2.11.1",
    "css-mqpacker": "^5.0.1",
    "cssnext": "^1.8.4",
    "csswring": "^5.1.0",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-base64": "^0.1.3",
    "gulp-cache": "^0.4.3",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.2",
    "gulp-load-plugins": "^1.2.0",
    "gulp-newer": "^1.1.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.0",
    "gulp-sass": "^2.2.0",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "gulp.spritesmith": "^6.2.0",
    "imagemin-pngquant": "^5.0.0",
    "node-notifier": "^4.6.0",
    "run-sequence": "^1.1.5"
  }
}
