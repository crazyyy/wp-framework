{
  "name": "wp-framework",
  "version": "2025.09.22",
  "description": "WordPress Bootstrap for Template Development with Gulp",
  "wp_site_name": "wp-framework.local",
  "wp_theme_name": "wp-framework",
  "homepage": "https://github.com/crazyyy/wp-framework",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development gulp serve",
    "build": "cross-env NODE_ENV=production gulp serve",
    "serve:test": "cross-env NODE_ENV=test gulp serve",
    "test": "npm run serve:test",
    "tasks": "gulp --tasks"
  },
  "browser": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "lintOptions": {
    "globals": {
      "React": false
    },
    "esnext": true,
    "node": true,
    "bitwise": true,
    "browser": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "immed": true,
    "indent": 2,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "strict": false,
    "globalstrict": true,
    "sub": true,
    "supernew": true,
    "undef": true,
    "unused": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crazyyy/wp-framework.git"
  },
  "bugs": {
    "url": "https://github.com/crazyyy/wp-framework/issues"
  },
  "main": "html/index.html",
  "author": {
    "name": "Vitalii A.",
    "email": "crazyyy@gmail.com",
    "url": "https://github.com/crazyyy"
  },
  "keywords": [
    "HTML", "CSS", "PHP", "WordPress", "ES5", "ES6", "JS", "Gulp", "JavaScript", "Sass", "Bootstrap", "RFS", "Responsive", "Web Development", "Frontend", "Front-end", "Template", "Theme", "Framework", "Starter"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
