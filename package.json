  "bugs": {
    "url": "https://github.com/crazyyy/wp-framework/issues"
  },
  "main": "html/index.html",
  "keywords": [
    "html",
    "css",
    "wordpress",
    "ES5",
    "ES6",
    "JS",
    "Gulp"
  ],
  "browser": {},
  "scripts": {
    "start": "SET NODE_ENV=development & gulp serve",
    "build": "SET NODE_ENV=production & gulp serve"
  },
  "lintOptions": {
    "globals": {
      "React": false
    },
    "esnext": true,
    "node": true,
    "bitwise": true,
    "browser": true,
    "curly": false,
