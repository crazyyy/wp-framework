  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "endOfLine": "auto"
  },
  "husky": {
    "hooks": {
      "disabled--pre-commit": "lint-staged",
      "disabled--pre-push": "yarn lint && yarn test",
      "disabled--pre-publish": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
